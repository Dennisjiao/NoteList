--A1L5 is Mux~86
--operation mode is normal

A1L5 = b & (a # c) # !b & a & c;


--A1L6 is Mux~87
--operation mode is normal

A1L6 = b $ a $ c;


--b is b
--operation mode is input

b = INPUT();


--a is a
--operation mode is input

a = INPUT();


--c is c
--operation mode is input

c = INPUT();


--ci is ci
--operation mode is output

ci = OUTPUT(A1L5);


--si is si
--operation mode is output

si = OUTPUT(A1L6);


